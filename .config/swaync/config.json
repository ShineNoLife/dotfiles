{
  "$schema": "/etc/xdg/swaync/configSchema.json",
  "positionX": "right",
  "positionY": "top",
  "layer": "overlay",
  "control-center-layer": "top",
  "layer-shell": true,
  "cssPriority": "user",
  "notification-2fa-action": true,
  "notification-inline-replies": false,
  "timeout": 6,
  "timeout-low": 3,
  "timeout-critical": 0,
  "control-center-width": 380,
  "control-center-height": 800,
  "keyboard-shortcuts": true,
  "transition-time": 200,
  "hide-on-clear": false,
  "hide-on-action": true,
  "script-fail-notify": true,
  "widgets": [
      "buttons-grid",
      "mpris",
      "volume",
      "backlight",
      "title",
      "notifications"
  ],
  "widget-config": {
      "title": {
          "text": "Notifications",
          "clear-all-button": true,
          "button-text": "󰎟"
      },
      "dnd": {
          "text": "Do Not Disturb"
      },
      "label": {
          "max-lines": 1,
          "text": "Notification"
      },
      "backlight": {
        "label": "󰃠",
        "device": "intel_backlight",
        "subsystem": "backlight",
        "min": 5
    }
  }
}